# Social Post Helper - Environment Variables
# Copy this file to .env and fill in your values

# Google Fonts API Key (Optional)
# Get your API key at: https://console.cloud.google.com/apis/credentials
# If not provided, the app will use a fallback list of popular fonts
VITE_GOOGLE_FONTS_API_KEY=your_google_fonts_api_key_here

# App Configuration (Optional)
VITE_APP_NAME="Social Post Helper"
VITE_MAX_STORAGE_MB=5

# Feature Flags (Optional)
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
